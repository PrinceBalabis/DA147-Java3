package laboration1a;

public class House {

	private double area;
	private int nbrOfRooms;
	
	public double getArea() {
		return area;
	}
	
	public void setArea(double area) {
		this.area = area;
	}
	
	public int getNbrOfRooms() {
		return nbrOfRooms;
	}
	
	public void setNbrOfRooms(int nbrOfRooms) {
		this.nbrOfRooms = nbrOfRooms;
	}

}
