package model;

public class Vara {
	private String namn;
	private float varuID, antal, kategoriID, pris, betyg;
	
	public Vara(String namn, float varuID, float antal, float kategoriID, float pris, float betyg) {
		super();
		this.namn = namn;
		this.varuID = varuID;
		this.antal = antal;
		this.kategoriID = kategoriID;
		this.pris = pris;
		this.betyg = betyg;
	}

	public String getNamn() {
		return namn;
	}

	public void setNamn(String namn) {
		this.namn = namn;
	}

	public float getVaruID() {
		return varuID;
	}

	public void setVaruID(float varuID) {
		this.varuID = varuID;
	}

	public float getAntal() {
		return antal;
	}

	public void setAntal(float antal) {
		this.antal = antal;
	}

	public float getKategoriID() {
		return kategoriID;
	}

	public void setKategoriID(float kategoriID) {
		this.kategoriID = kategoriID;
	}

	public float getPris() {
		return pris;
	}

	public void setPris(float pris) {
		this.pris = pris;
	}

	public float getBetyg() {
		return betyg;
	}

	public void setBetyg(float betyg) {
		this.betyg = betyg;
	}
	
	
}
