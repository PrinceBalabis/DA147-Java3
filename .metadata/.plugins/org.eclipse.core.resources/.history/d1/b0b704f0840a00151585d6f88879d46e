package p2;

public class SimpleDice implements Dice{
	private int sides;
	
	public SimpleDice(){
		sides = 1;
	}
	
	public SimpleDice(int sides){
		if(sides < 1)
			this.sides = 1;
			
	}
	
	@Override
	public int throwDice() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int getSides() {
		// TODO Auto-generated method stub
		return 0;
	}

}
